   "source": [
    "#Removing '$' from the Per_Capita_Income_Zipcode column and converting it to float data type\n",
    "df_cleaned = df.withColumn('Per_Capita_Income_Zipcode', F.regexp_replace(F.col('Per_Capita_Income_Zipcode'), '[$,]', ''))"
   ]
  },
  {
   "cell_type": "code",
