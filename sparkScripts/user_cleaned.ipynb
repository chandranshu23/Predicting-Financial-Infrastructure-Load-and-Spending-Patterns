   "id": "2829a4fb",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Removing '$' from the Yearly_Income_Person column and converting it to float data type\n",
    "df_cleaned = df_cleaned.withColumn('Yearly_Income_Person', F.regexp_replace(F.col('Yearly_Income_Person'), '[$,]', ''))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "3075b3c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Removing '$' from the Total_Debt column and converting it to float data type\n",
    "df_cleaned = df_cleaned.withColumn('Total_Debt', F.regexp_replace(F.col('Total_Debt'), '[$,]', ''))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "74a25feb",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Casting the Per_Capita_Income_Zipcode cloumn to Float(10,2)\n",
    "df_cleaned = df_cleaned.withColumn(\n",
    "    'Per_Capita_Income_Zipcode',\n",
    "    F.col('Per_Capita_Income_Zipcode').cast(DecimalType(10, 2))\n",
    ")"
   ]
  },
